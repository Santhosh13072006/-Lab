#include<stdio.h>
int main()
{
    int a,b,c;
    printf("enter a:");
    scanf("%d",&a);
    printf("enter b:");
    scanf("%d",&b);
    printf("enter c:");
    scanf("%d",&c);
    if(a>b)
    {
        if(a>c)
        {
            printf("a");
        }
        else
        {
            printf("c");
        }
    }
    else if(b>a)
    {
        if(b>c)
         {
            printf("b");
        }
        else
        {
            printf("c");
        }
    }
    return 0;
}
